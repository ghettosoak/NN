<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<title>NN</title>
		<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
		<link href="/css/main.css" rel="stylesheet">
		{{{isAdminEditorCSS user}}}
	</head>
	<body id="body" class="" viewing-category="">
		{{{body}}}
		<div id="lander">
			<div id="lander_interior">
				<div id="lander_nav">
					<div class="prev">
						<div class="lander_nav_interior">
							<span class="direction">prev</span>
							<span class="project_title_lander"></span>
						</div>
					</div>
					<div class="next">
						<div class="lander_nav_interior">
							<span class="direction">next</span>
							<span class="project_title_lander"></span>
						</div>
					</div>
				</div>
				<div id="lander_target"></div>
			</div>
		</div>
		<div id="loader">
			<div class="loader_exterior">
				<div class="loader_interior">
					<div id="code">
						<div id="code_interior"></div>
					</div>
					<div class="loader_icon eternal">
						<div class="loader_icon_interior"><span></span></div>
					</div>
				</div>
			</div>
		</div>
		{{{isAdminEditorJS user}}}
		<script type="text/javascript" src="js/main.js"></script>
	</body>
</html>
